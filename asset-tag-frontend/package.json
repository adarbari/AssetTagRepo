            "@types/node": "^20.10.0",
            "@types/react": "^18.3.1",
            "@types/react-dom": "^18.3.1",
            "@typescript-eslint/eslint-plugin": "^8.46.0",
            "@typescript-eslint/parser": "^6.21.0",
            "@vitejs/plugin-react": "^5.0.4",
            "@vitest/coverage-v8": "^3.2.4",
            "@vitest/ui": "^3.2.4",
            "eslint": "^8.55.0",
            "eslint-plugin-jsx-a11y": "^6.10.2",
            "eslint-plugin-react": "^7.37.5",
            "eslint-plugin-react-hooks": "^4.6.2",
            "jsdom": "^25.0.1",
            "prettier": "^3.1.0",
            "tsx": "^4.7.0",
            "typescript": "^5.3.0",
            "vite": "^6.3.6",
            "vitest": "^3.2.4"
      },
      "scripts": {
            "dev": "vite",
            "build": "vite build",
            "test": "vitest",
            "test:ui": "vitest --ui",
            "test:run": "vitest run",
            "test:coverage": "vitest run --coverage",
            "test:coverage:open": "vitest run --coverage && open coverage/index.html",
            "test:coverage:ci": "vitest run --coverage --reporter=verbose --reporter=json --outputFile=coverage/coverage-summary.json",
            "test:ci": "vitest run --coverage --reporter=verbose --bail=5 --pool=threads --poolOptions.threads.maxThreads=2",
            "test:critical": "tsx src/test/run-critical-tests.ts",
            "test:sites": "vitest run src/components/__tests__/Sites.test.tsx src/components/__tests__/SiteDetails.test.tsx src/components/__tests__/CreateSite.test.tsx",
            "test:map": "vitest run src/components/__tests__/AssetMap.test.tsx",
            "test:dashboard": "vitest run src/components/__tests__/Dashboard.test.tsx",
            "test:maintenance": "vitest run src/components/__tests__/Maintenance.test.tsx",
            "test:vehicle-pairing": "vitest run src/components/__tests__/VehicleAssetPairing.test.tsx",
            "test:integration": "vitest run src/test/integration/__tests__/",
            "test:watch": "vitest --watch",
            "test:watch:ui": "vitest --watch --ui",
            "test:new": "vitest run src/components/__tests__/Reports.test.tsx src/utils/__tests__/errorHandler.test.ts src/services/__tests__/api.test.ts src/hooks/__tests__/useAsyncData.test.ts src/test/integration/__tests__/userFlow.test.tsx",
            "test:debug": "vitest run --reporter=verbose --no-coverage --poolOptions.threads.maxThreads=2",
            "test:single-thread": "vitest run --poolOptions.threads.singleThread=true",
            "test:components": "vitest run src/components/__tests__/ --poolOptions.threads.maxThreads=2",
            "test:services": "vitest run src/services/__tests__/ --poolOptions.threads.maxThreads=2",
            "test:utils": "vitest run src/utils/__tests__/ --poolOptions.threads.maxThreads=2",
            "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
            "lint:fix": "eslint src --ext .ts,.tsx --fix",
            "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
            "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
            "type-check": "tsc --noEmit",
            "type-check:watch": "tsc --noEmit --watch",
            "postinstall": "npm run audit-check || true",
            "audit-check": "npm audit --audit-level=high"
      }
}
